#string slicing concept 
#input string assignment
var = "Google_chrome_browser"

#print the output
print(var[-10:10:3])
print(var[-5:-1:1])
print(var[-8:-2:10])
print(var[5::3])
print(var[:5:2])
print(var[10:14:2])
print(var[-10:-3:3])
print(var[5:12:5])
print(var[3:-3:3])
print(var[-5:-3:20])

