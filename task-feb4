#string slicing concept 
#input string assignment
var = "computer science"

#print the output
print(var[-10:10:3])
print(var[-5:-1:1])
print(var[-8:-2:10])
print(var[5::3])
print(var[:5:2])
print(var[10:14:2])
print(var[-10:-3:3])
print(var[5:12:5])
print(var[3:-3:3])
print(var[-5:-3:20])

#output
es
ienc
empty
t ic
cmu
ce
ese
tc
pese
i

#string slicing concept 
#input string assignment
var = "information_Technology"

#print the output
print(var[-10:10:3])
print(var[-5:-1:1])
print(var[-8:-2:10])
print(var[5::3])
print(var[:5:2])
print(var[10:14:2])
print(var[-10:-3:3])
print(var[5:12:5])
print(var[3:-3:3])
print(var[-5:-3:20])

